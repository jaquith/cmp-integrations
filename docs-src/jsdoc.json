{
  "tags": {
    "allowUnknownTags": ["category"]
  },
  "source": {
      "include": ["./extensions"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/category"
  ],
  "opts": {
      "encoding": "utf8",
      "destination": "./docs/",
      "readme": "./docs-src/doc-home.md",
      "recurse": true,
      "verbose": true,
      "template": "./node_modules/docdash/"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "search": false,
    "default": {
        "staticFiles": {
          "include": [ "./docs-src/static/" ]
        }
    },
    "docdash": {
      "static": true,
      "sort": true,
      "search": true,
      "typedefs": true,
      "navLevel": 3
    }
  }
}

